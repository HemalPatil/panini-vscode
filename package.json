{
    "name": "panini-vscode",
    "publisher": "hemalpatil",
    "author": {
        "name": "Hemal Patil",
        "url": "https://github.com/HemalPatil"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/HemalPatil/panini-vscode.git"
    },
    "displayName": "Panini",
    "description": "Panini Syntax Highlighting",
    "version": "0.0.4",
    "engines": {
        "vscode": "^1.57.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "panini",
            "aliases": ["पाणिनी", "panini"],
            "extensions": [".ण", "panini"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "panini",
            "scopeName": "source.panini",
            "path": "./syntaxes/panini.tmLanguage.json"
        }]
    }
}